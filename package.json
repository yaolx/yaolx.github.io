{
  "name": "yaolx.github.io",
  "version": "0.0.1",
  "homepage": "https://yaolx.github.io/",
  "author": "千禮之行",
  "scripts": {
    "dev": "vite -m test",
    "serve": "vite preview",
    "start:mock": "vite -m mock",
    "start:test": "vite -m test",
    "start:production": "vite -m production",
    "build": "tsc && vite build && yarn copy",
    "build:test": "tsc && vite build -m test && yarn copy",
    "build:production": "tsc && vite build -m production",
    "copy": "copyfiles -f .nojekyll dist",
    "lint": "eslint src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@yaolx/utils": "^1.0.1",
    "antd": "^4.20.2",
    "axios": "^0.23.0",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "lodash": "^4.17.21",
    "markmap-lib": "^0.13.3",
    "markmap-view": "^0.13.3",
    "mobx": "^6.3.5",
    "mobx-react": "^7.2.0",
    "mockjs": "^1.1.0",
    "prism-react-renderer": "1.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@rollup/plugin-eslint": "8.0.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^1.3.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "husky": "4.3.8",
    "less": "^4.1.2",
    "lint-staged": "^11.2.3",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "^2.4.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.3.2",
    "vconsole": "^3.14.6",
    "vite": "^2.6.4",
    "vite-plugin-chunk-split": "^0.4.0",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-mdx": "^3.5.10",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^1.2.1"
  }
}
